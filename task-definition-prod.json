                    "value": "production"
                },
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "fanc2023"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "fancroot"
                },
                {
                    "name": "SENDGRID_API_KEY",
                    "value": "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"
                },
                {
                    "name": "JWT_SECRET_KEY",
                    "value": "KbHO6UrJHDxIeGwRGw8unB4dEDLn8fNpXY5uFYwfup8"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/fanc-api-task-definition-prod",
                    "awslogs-region": "us-east-1",
